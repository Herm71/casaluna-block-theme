{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"customTemplates": [
		{
			"name": "page-with-full-width",
			"postTypes": [ "post", "page" ],
			"title": "Page With Full Width"
		},
		{
			"name": "page-with-left-sidebar",
			"postTypes": [ "post", "page" ],
			"title": "Page With Left Sidebar"
		},
		{
			"name": "page-with-right-sidebar",
			"postTypes": [ "post", "page" ],
			"title": "Page With Right Sidebar"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": [ "#00a0d2", "#0073aa" ],
					"name": "Secondary and primary",
					"slug": "secondary-primary"
				},
				{
					"colors": [ "#0073aa", "#000000" ],
					"name": "Primary and Contrast",
					"slug": "primary-contrast"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(#0073aa 49.9%,#fff 50%)",
					"name": "Primary color to white",
					"slug": "primary-white"
				},
				{
					"gradient": "linear-gradient(#fff 49.9%,#0073aa 50%)",
					"name": "Horizontal white to Primary color",
					"slug": "white-primary"
				}
			],
			"palette": [
				{
					"color": "#14212c",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#ffffff",
					"name": "Contrast",
					"slug": "bright"
				},
				{
					"color": "#E8EDE7",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#1f738b",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#3a9cb5",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#b7675d",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#e59084",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "transparent",
					"name": "Transparent",
					"slug": "transparent"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1280px"
		},
		"spacing": {
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Oswald",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/oswald/Oswald-Regular.woff2"
							]
						}
					],
					"fontFamily": "\"Oswald\", serif",
					"name": "Oswald",
					"slug": "Oswald"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/open-sans/open-sans-v35-latin-regular.woff2"
							]
						}
					],
					"fontFamily": "\"Open Sans\", serif",
					"name": "Open Sans",
					"slug": "open-sans"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Extra small",
					"size": "1rem",
					"slug": "extra-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "1.125rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.25rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.75rem"
					},
					"name": "Large",
					"size": "2rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.75rem",
						"min": "2.5rem"
					},
					"name": "XL",
					"size": "2.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3.75rem",
						"min": "3rem"
					},
					"name": "XXL",
					"size": "3.75rem",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"textDecoration": "none"
				}
			},
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-small)"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/query-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--tertiary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--bright)",
			"text": "var(--wp--preset--color--base)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--bright)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--bright)"
				},
				"shadow": "var(--wp--preset--shadow--natural)",
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				":active": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--tertiary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"padding": {
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "sidebar",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"version": 2
}
